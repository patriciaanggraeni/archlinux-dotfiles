{
    "layer": "top",
    "position": "top",
    "height": 30,
    "margin": "5 10 5 10", // atas kanan bawah kiri
    "spacing": 8,
    "output": ["eDP-1", "HDMI-A-1"], // tampil di laptop dan monitor eksternal
    "exclusive": true,
    "fixed-center": true,
    "start_hidden": false,
    "modules-left": ["custom/launcher", "sway/workspaces"],
    "modules-center": ["clock"],
    "modules-right": ["pulseaudio", "battery", "network", "tray"],
    "name": "mainbar",
    "reload_style_on_change": true,
    "include": [
      "~/.config/waybar/modules/network.json"
      // "~/.config/waybar/modules/clock.jsonc"
    ]
}